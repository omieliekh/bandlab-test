{
  "name": "angular-webpack-seed",
  "version": "0.1.0",
  "description": "angualr project template",
  "main": "index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "webpack --config webpack-production.config.js --progress --colors",
    "build-dev": "webpack --config webpack.config.js --progress --colors",
    "dev": "webpack-dev-server --config webpack-dev-server.config.js --progress --colors --port 2992 --inline --content-base docs",
    "test": "karma start",
    "cover": "rimraf coverage && COVERAGE=1 karma start",
    "lint": "eslint ./src/",
    "postprocess-css": "sed -i '' -e 's|url(/|url(|g' docs/main.css",
    "add-docs": "rm -rf docs/debugging && git add docs/",
    "precommit": "npm run build-dev && npm run postprocess-css && npm run add-docs"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "angular": "^1.6.4",
    "angular-route": "^1.6.5",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.4",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "codecov.io": "^0.1.6",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "husky": "^0.14.3",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.6.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.3",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "loader-utils": "^1.1.0",
    "ng-annotate-loader": "^0.6.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.0"
  }
}
